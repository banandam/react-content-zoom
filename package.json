{
  "name": "react-content-zoom",
  "version": "1.0.0",
  "description": "A pure boilerplate for developing components in React using webpack workflow",
  "main": "index.js",
  "scripts": {
    "build": "webpack --output-filename index.js",
    "clean": "npm-run-all --parallel clean:*",
    "clean:build": "rm -r run/build",
    "clean:dist": "rm -r dist",
    "copy": "babel --copy-files --out-dir dist --ignore bundle.js run/build/",
    "dev": "webpack-dev-server --hot --inline",
    "prebuild": "webpack",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "ssimplatina@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.2",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "6.11.6",
    "css-loader": "^0.23.1",
    "eslint": "^3.0.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-node-externals": "^1.6.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}